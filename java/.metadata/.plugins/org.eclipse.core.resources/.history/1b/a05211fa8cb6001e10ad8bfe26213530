package com.test.java.collection;

import java.util.Arrays;

public class Ex61_Sort {
	public static void main(String[] args) {
		//Ex61_Sort.java
		
		//배열 or 컬렉션 
		//- 순서가 있는 데이터 집합 
		//- 정렬 사용
		
		//- 정렬
		//- 오르차순, 내림차순
		//1. 직접 구현 > 정렬 알고리즘
		//2. JDK 제공 기능 > Arrays.sort()
		
		m1();
	}

	private static void m1() {
		//1. 숫자 
		//2. 문자(열)
		//3. 날짜시간 
		
		//A.1 숫자 + 배열 
		int [] nums1 = {1,5,3,4,3};
		
		System.out.println(Arrays.toString(nums1));
		
		Arrays.sort(nums1);
		
		System.out.println(Arrays.toString(nums1));
		System.out.println();
		
		
	}
}

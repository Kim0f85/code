package com.test.question;

public class Q059 {
	public static void main(String[] args) {
		int result = 1; 
		int addition1 = 0;
		int addition2 = 1;
		int num1 = 0;
		for(int i = 1; i<100 ; i++) {
			if (result<100) {
				result+=incre(i);
				System.out.printf("%d + ",result);
			}
		}
	}

	private static int incre(int i) {
		int make = 0;
		for(int j = 0 ; j <i;j++) {
			make++;
		}
		return make;
	}
}

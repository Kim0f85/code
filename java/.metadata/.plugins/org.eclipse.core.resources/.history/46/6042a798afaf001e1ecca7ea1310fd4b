package com.test.question;

public class Q057 {
	public static void main(String[] args) {
		
		//int count = 0; 

		for (int i = 0; i <= 100; i+=10) {
			int count = 0; 
			int num1 = 0;
			for (int j = 0; j<=i;j++) {
				count += count + j;
				if (i==j) {
					System.out.printf("1 ~ %d: %4d",j,count);
				}
			}
		}
	}
}

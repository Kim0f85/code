package com.test.question;

import java.util.Scanner;

public class Q055 {
	public static void main(String[] args) {

		int input = 100;
		int output = 0;
		String s1 = "";
		int n1 = 0;
		int n2 = 0;

		for (int i = 1; i <= input; i++) {
			output = 0;

			for (int j = 1; j < i; j++) {

				if (i % j == 0 && output <= i) {
					output += j;
					s1 = s1+ j+",";
				} 
			}
			if (output == i) {
				System.out.printf("%d = [%s]\n",output,s1);
			}
		}

	}

}

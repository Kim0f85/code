package com.test.question;

import java.util.Scanner;

public class Q055 {
	public static void main(String[] args) {

		int input = 5;
		int output = 0;
		String s1 = "";
		int n1 = 0;
		int n2 = 0;

		for (int i = 1; i <= input; i++) {
			for (int j = 1; j <= i; j++) {
				output = 0;
				if (i%j==0 && output <=i) {
					output+=j;
					
				}else {
					n1++;
				}
			}
			if(output==i) {
				System.out.println(output);
			}
		}

	}

}

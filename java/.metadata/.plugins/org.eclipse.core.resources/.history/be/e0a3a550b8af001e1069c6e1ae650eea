package com.test.question;

public class Q060 {
	public static void main(String[] args) {
		
		int addition1 = 1;
		int addition2 = 1;
		int result =0;
		System.out.printf("1 + 1 + ");
		while (result <100) {
			
			result = addition1 +addition2;
			addition1 = result;
			System.out.printf("%d + ",result);
			result = addition2 + addition1;
			addition2= result;
			System.out.printf("%d + ",result);
		}
		
	}
}
